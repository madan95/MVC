@model IEnumerable<Research.Models.Links>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_ULayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ChairWebsite)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CoChairWebsite)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ChairWebsite)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CoChairWebsite)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.LinksID }) |
            @Html.ActionLink("Details", "Details", new { id=item.LinksID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.LinksID })
        </td>
    </tr>
}

</table>
